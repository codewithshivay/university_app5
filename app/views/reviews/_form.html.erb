<%= form_with(model: [@course, Review.new], local: true) do |form| %>
  <div class="input-field">
    <%= form.label :content, class: "active" %>
    <%= form.text_field :content, class: "validate", placeholder: "Enter Your Review" %>
  </div>
  <div class="input-field">
    <%= form.submit 'Add Review', class: "btn waves-effect waves-light" %>
  </div>
<% end %>
